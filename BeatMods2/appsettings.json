{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DBMode": "postgres",
  "GithubAuth": {
    "OauthScopes": [
      "repo:status",
      "repo_deployment"
    ]
  },
  "CoreAuth": {
    "JwtExpiry": "7"
  },
  "JwtSettings": {
    "RequireHttpsMetadata": false,
    "SaveToken": true,
    "TokenValidationParameters": {
      "ValidateIssuerSigningKey": true,
      "ValidateIssuer": true,
      "ValidateAudience": false
    }
  }
}
